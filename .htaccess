<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteRule ^assets/ - [L,NC]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_URI} !=/favicon.ico
    RewriteRule ^ koseu.php [L]
</IfModule>

<IfModule !mod_rewrite.c>
    FallbackResource koseu.php
</IfModule>
